<body>
 <h1>Edit Skill</h1>
 <form action="/skills/<%= skillId %>?_method=PUT" method="POST">
    <input type="text" name="skill" autocomplete="off"
    value="<%= skill.skill %>">
    <label>
        <input name="have" <%= skill.have ? : 'checked' : '' %>>         1.43 youtube
        Have
    </label>
    <button type="submit">Update Skill</button>
</form>

</body>
</body>
